doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Movies
        .cards 
            each movie in data
                .card 
                    h2.card__name #{contact.properties.name}
                    p.card__email #{contact.properties.genre} - #{contact.properties.rating}